    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "pyoncokb"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "tests all",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest"
        },
        {
            "name": "tests.chromacmapper",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": [
                "discover",
                "tests.chromacmapper"
            ],
        },
        {
            "name": "tests.converters",
            "type": "debugpy",
